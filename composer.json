{
  "name": "ergebnis/clock",
  "description": "Provides abstractions of a clock.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "clock",
    "system-clock",
    "test-clock"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/clock",
  "support": {
    "issues": "https://github.com/ergebnis/clock/issues",
    "source": "https://github.com/ergebnis/clock"
  },
  "require": {
    "php": "~7.0.0 || ~7.1.0 || ~7.2.0 || ~7.3.0 || ~7.4.0 || ~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0",
    "psr/clock": "^1.0.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.44.0",
    "ergebnis/license": "^2.5.0",
    "ergebnis/php-cs-fixer-config": "^6.37.0",
    "infection/infection": "~0.26.6",
    "phpunit/phpunit": "^6.5.14 || ^7.5.20 || ^8.5.19 || ^9.6.16",
    "psalm/plugin-phpunit": "~0.19.0",
    "rector/rector": "^1.2.5",
    "vimeo/psalm": "^5.26.1"
  },
  "provide": {
    "psr/clock-implementation": "1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Clock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Clock\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "7.4.33"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
