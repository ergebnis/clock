{
  "name": "ergebnis/clock",
  "description": "Provides a simple abstraction of a clock.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "clock",
    "system-clock",
    "test-clock"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/clock",
  "support": {
    "issues": "https://github.com/ergebnis/clock/issues",
    "source": "https://github.com/ergebnis/clock"
  },
  "require": {
    "php": "^8.0",
    "psr/clock": "^1.0.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.30.2",
    "ergebnis/license": "^2.1.0",
    "ergebnis/php-cs-fixer-config": "~5.3.3",
    "infection/infection": "~0.26.19",
    "phpunit/phpunit": "^9.6.5",
    "psalm/plugin-phpunit": "~0.18.4",
    "vimeo/psalm": "^5.8.0"
  },
  "provide": {
    "psr/clock-implementation": "1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Clock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Clock\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.0.24"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
