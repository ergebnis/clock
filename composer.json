{
  "name": "ergebnis/clock",
  "description": "Provides abstractions of a clock.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "clock",
    "system-clock",
    "test-clock"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/clock",
  "support": {
    "issues": "https://github.com/ergebnis/clock/issues",
    "source": "https://github.com/ergebnis/clock"
  },
  "require": {
    "php": "~7.4.0 || ~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0 || ~8.5.0",
    "psr/clock": "^1.0.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.48.1",
    "ergebnis/license": "^2.6.0",
    "ergebnis/php-cs-fixer-config": "^6.53.0",
    "infection/infection": "~0.26.6",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.22",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpstan/phpstan-phpunit": "^2.0.7",
    "phpstan/phpstan-strict-rules": "^2.0.6",
    "phpunit/phpunit": "^6.5.14 || ^7.5.20 || ^8.5.19 || ^9.6.16",
    "rector/rector": "^2.1.4"
  },
  "provide": {
    "psr/clock-implementation": "1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Clock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Clock\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "7.4.33"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.10-dev"
    },
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
