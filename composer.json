{
  "name": "ergebnis/clock",
  "description": "Provides a simple abstraction of a clock.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "clock",
    "system-clock",
    "test-clock"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/clock",
  "support": {
    "issues": "https://github.com/ergebnis/clock/issues",
    "source": "https://github.com/ergebnis/clock"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
    "psr/clock": "^1.0.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.41.1",
    "ergebnis/license": "^2.4.0",
    "ergebnis/php-cs-fixer-config": "~6.15.0",
    "ergebnis/phpunit-slow-test-detector": "^2.9.0",
    "infection/infection": "~0.27.9",
    "phpunit/phpunit": "^10.5.3",
    "psalm/plugin-phpunit": "~0.18.4",
    "rector/rector": "~0.18.13",
    "roave/backward-compatibility-check": "^8.6.0",
    "vimeo/psalm": "^5.18.0"
  },
  "provide": {
    "psr/clock-implementation": "1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Clock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Clock\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.1.26"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
