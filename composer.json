{
  "name": "ergebnis/clock",
  "type": "library",
  "description": "Provides a simple abstraction of a clock.",
  "keywords": [
    "clock",
    "system-clock",
    "test-clock"
  ],
  "homepage": "https://github.com/ergebnis/clock",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.14.0",
    "ergebnis/license": "^1.1.0",
    "ergebnis/php-cs-fixer-config": "^2.13.1",
    "ergebnis/phpstan-rules": "~0.15.3",
    "ergebnis/test-util": "^1.5.0",
    "infection/infection": "~0.15.3",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "~0.12.84",
    "phpstan/phpstan-deprecation-rules": "~0.12.6",
    "phpstan/phpstan-strict-rules": "~0.12.9",
    "phpunit/phpunit": "^8.5.15",
    "psalm/plugin-phpunit": "~0.15.1",
    "vimeo/psalm": "^4.7.0"
  },
  "config": {
    "platform": {
      "php": "7.2.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Clock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Clock\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/ergebnis/clock/issues",
    "source": "https://github.com/ergebnis/clock"
  }
}
